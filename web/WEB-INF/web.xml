<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>controller.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>controller.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>submitToDatabase</servlet-name>
        <servlet-class>controller.submitToDatabase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchmovie</servlet-name>
        <servlet-class>controller.searchmovie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteFromDatabase</servlet-name>
        <servlet-class>controller.deleteFromDatabase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadimg</servlet-name>
        <servlet-class>controller.uploadimg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendEmail</servlet-name>
        <servlet-class>controller.sendEmail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>submitToDatabase</servlet-name>
        <url-pattern>/submitToDatabase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchmovie</servlet-name>
        <url-pattern>/searchmovie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteFromDatabase</servlet-name>
        <url-pattern>/deleteFromDatabase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadimg</servlet-name>
        <url-pattern>/uploadimg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendEmail</servlet-name>
        <url-pattern>/sendEmail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>oms_01_home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
